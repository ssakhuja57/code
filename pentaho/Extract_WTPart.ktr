    <sql>select m.wtpartnumber as part_number, m.classnamekeycontainerreferen as part_conttype, cont.namecontainerinfo as part_contname, org.name as part_org, 
d.versionida2versioninfo as part_revision, d.iterationida2iterationinfo as part_iteration
from wtpartmaster m, wtpart d, pdmlinkproduct cont,wtorganization org
where m.ida2a2=d.ida3masterreference
and m.ida3containerreference=cont.ida2a2
and d.latestiterationinfo=1
and d.statestate=&apos;RELEASED&apos;
and m.ida3organizationreference=org.ida2a2

union all

select m.wtpartnumber as part_number, m.classnamekeycontainerreferen as part_conttype, cont.namecontainerinfo as part_contname, org.name as part_org,  
d.versionida2versioninfo as part_revision, d.iterationida2iterationinfo as part_iteration
from wtpartmaster m, wtpart d, wtlibrary cont,wtorganization org
where m.ida2a2=d.ida3masterreference
and m.ida3containerreference=cont.ida2a2
and d.latestiterationinfo=1
and d.statestate=&apos;RELEASED&apos;
and m.ida3organizationreference=org.ida2a2
