(SELECT IBAV.VALUE2 FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.chainring') AS "PMD_CHAINRING",
(SELECT IBAV.VALUE2 FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.cog') AS "PMD_COG",
(SELECT LISTAGG(IBAV.VALUE2,',') WITHIN GROUP (ORDER BY IBAV.VALUE) FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.colors') AS "PMD_COLORS",
(SELECT LISTAGG(IBAV.VALUE2,',') WITHIN GROUP (ORDER BY IBAV.VALUE) FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.compare_to') AS "PMD_COMPARE_TO",
(SELECT IBAV.VALUE FROM FLOATDEFINITION IBAD, FLOATVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.cost_pnt') AS "PMD_COST_PNT",
(SELECT IBAV.VALUE FROM FLOATDEFINITION IBAD, FLOATVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.cost_pnt_comp') AS "PMD_COST_PNT_COMP",
(SELECT IBAV.VALUE FROM FLOATDEFINITION IBAD, FLOATVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.cost_pnt_fork') AS "PMD_COST_PNT_FORK",
(SELECT IBAV.VALUE FROM FLOATDEFINITION IBAD, FLOATVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.cost_unpnt') AS "PMD_COST_UNPNT",
(SELECT IBAV.VALUE FROM FLOATDEFINITION IBAD, FLOATVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.cost_unpnt_comp') AS "PMD_COST_UNPNT_COMP",
(SELECT IBAV.VALUE FROM FLOATDEFINITION IBAD, FLOATVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.cost_unpnt_fork') AS "PMD_COST_UNPNT_FORK",
(SELECT IBAV.VALUE2 FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.crank') AS "PMD_CRANK",
(SELECT IBAV.VALUE2 FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.fork') AS "PMD_FORK",
(SELECT IBAV.VALUE FROM FLOATDEFINITION IBAD, FLOATVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.fork_travel') AS "PMD_FORK_TRAVEL",
(SELECT IBAV.VALUE2 FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.frm_config') AS "PMD_FRM_CONFIG",
(SELECT IBAV.VALUE2 FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.front_der') AS "PMD_FRONT_DER",
(SELECT IBAV.VALUE2 FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.gender') AS "PMD_GENDER",
(SELECT IBAV.VALUE2 FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.headset') AS "PMD_HEADSET",
(SELECT IBAV.VALUE2 FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.metric') AS "PMD_METRIC",
(SELECT LISTAGG(IBAV.VALUE2,',') WITHIN GROUP (ORDER BY IBAV.VALUE) FROM STRINGDEFINITION IBAD, STRINGVALUE IBAV where D.IDA2A2 = IBAV.IDA3A4 AND IBAV.IDA3A6 = IBAD.IDA2A2 AND IBAD.NAME = 'local.default.misc') AS "PMD_MISC"
from wtpartmaster M join wtpart D on M.ida2a2 = D.ida3masterreference
and D.latestversioninfo = '1';
